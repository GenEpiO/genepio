        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GEO_000000391"/>
        <obo:IAO_0000115 xml:lang="en">a geopolitical organization that is voluntary and private, whose members are individual persons or organizations that come together to acheive a common purpose.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/geo.owl"/>
