    <!-- http://purl.obolibrary.org/obo/ARO_3004410 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/ARO_3004410">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">micro broth dilution method</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ARO_3004397"/>
