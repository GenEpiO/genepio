        <rdfs:subClassOf rdf:resource="&obo;PO_0025496"/>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">A multi-tissue plant structure (PO:0025496) that develops from a plant ovule (PO:0020003) and has as parts a plant embryo (PO:0009009) enclosed in a seed coat (PO:0009088).</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="&obo;po.owl"/>
        <rdfs:label rdf:datatype="&xsd;string">seed</rdfs:label>
