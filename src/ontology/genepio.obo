property_value: IAO:0000119 "https://www.ebi.ac.uk/ols/ontologies/trans" xsd:anyURI

[Term]
id: TRANS:0000007
name: contact
def: "Contact transmission is a direct transmission process during which the pathogen is transmitted from a reservoir, source or host to another host by kissing, skin-to-skin contact, sexual intercourse, or by contact with soil or vegetation containing the pathogen." []
is_a: TRANS:0000001 ! direct transmission
property_value: GENEPIO:0000006 "contact transmission" xsd:string
property_value: IAO:0000119 "https://www.ebi.ac.uk/ols/ontologies/trans" xsd:anyURI

[Term]
