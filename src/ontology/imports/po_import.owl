    <!-- http://purl.obolibrary.org/obo/PO_0009001 -->

    <owl:Class rdf:about="&obo;PO_0009001">
        <rdfs:subClassOf rdf:resource="&obo;PO_0025496"/>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">A multi-tissue plant structure (PO:0025496) that develops from a gynoecium (PO:0009062), or a single carpel (PO:0009030), and at maturity may have as parts one or more seeds (PO:0009010).</obo:IAO_0000115>
